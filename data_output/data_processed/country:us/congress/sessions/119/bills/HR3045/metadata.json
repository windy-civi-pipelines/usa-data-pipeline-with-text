  "subject": [],
  "abstracts": [],
  "other_titles": [
    {
      "note": "",
      "title": "To prevent violence in the West Bank and authorize the imposition of sanctions with respect to any foreign person endangering United States national security and undermining prospects for a two-state solution by committing illegal violent acts."
    }
  ],
